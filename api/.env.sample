# Sample environment for the API
# Copy this file to `.env` and fill the values before running the API

# Node environment (development|production|test)
NODE_ENV=development

# Port where the API will listen
PORT=3333

# Postgres connection URL (example)
# Replace user:password, host and dbname accordingly
DATABASE_URL=postgres://user:password@localhost:5432/webhook_debugger

# Google Generative AI (Gemini) API key â€” required to use POST /api/generate
# Get this in Google Cloud and enable the Generative AI API
GOOGLE_GENERATIVE_AI_API_KEY=
